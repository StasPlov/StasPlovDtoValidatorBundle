{
    "name": "StasPlov/dto-validator-bundle",
	"type": "symfony-bundle",
    "description": "Adds validator for middleware request support in your Symfony application",
    "keywords": ["Symfony", "bundle", "dto", "validator", "validate", "api", "rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stas Plov",
			"email": "SaviouR.S@mail.ru",
            "homepage": "https://github.com/StasPlov"
        }
    ],
    "require": {
		"php": ">=8.2",
        "symfony/http-kernel": "^6.0|^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": { "StasPlov\\CorsBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}