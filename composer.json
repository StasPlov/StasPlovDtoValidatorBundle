{
    "name": "stas-plov/dto-validator-bundle",
	"version": "v1.1.3",
	"type": "symfony-bundle",
    "description": "Adds validator for middleware request support in your Symfony application",
    "keywords": ["Symfony", "bundle", "dto", "validator", "validate", "api", "rest"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stas Plov",
			"email": "SaviouR.S@mail.ru",
            "homepage": "https://github.com/StasPlov"
        }
    ],
    "require": {
		"php": ">=8.2",
		"psr/log": "^1.0 || ^2.0 || ^3.0",
		"symfony/config": "^6.0|^7.0",
		"symfony/dependency-injection": "^6.0|^7.0",
		"symfony/http-foundation": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
		"symfony/validator": "^6.0|^7.0",
		"symfony/serializer": "^6.0|^7.0",
        "symfony/property-info": "^6.0|^7.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.4 || ^6.0 || ^7.0"
    },
    "autoload": {
        "psr-4": { "StasPlov\\DtoValidatorBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
